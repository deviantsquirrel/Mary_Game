    \u0443\u0436\u043E \u0431\u044B\u043B\u0456."
  - "\u041F\u0430\u0436\u044B\u043B\u0430\u044F \u0434\u0430\u043C\u0430:\r\n\u0421\u0451\u043D\u043D\u044F
    \u0441\u0430\u043F\u0440\u0430\u045E\u0434\u043D\u0430\u0435 \u0430\u0441\u0430\u0431\u043B\u0456\u0432\u044B
    \u0434\u0437\u0435\u043D\u044C. \u041C\u0430\u044F \u0434\u0430\u0447\u043A\u0430
    \u043F\u0440\u044B\u044F\u0437\u0434\u0436\u0430\u0435 \u045E \u0433\u043E\u0441\u0446\u0456,
    \u0456 \u044F\u043D\u0430 \u043F\u0440\u044B\u0432\u043E\u0434\u0437\u0456\u0446\u044C
